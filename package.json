{
    "name": "lv2-datatable",
    "version": "0.1.0",
    "description": "Integrate Laravel Vue Datatable with your Laravel application without using JQuery.",
    "main": "dist/DataTable.ssr.js",
    "browser": "dist/DataTable.esm.js",
    "module": "dist/DataTable.esm.js",
    "unpkg": "dist/DataTable.min.js",
    "files": [
        "dist/*",
        "src/**/*.vue"
    ],
    "sideEffects": false,
    "scripts": {
        "serve": "vue-cli-service serve dev/serve.js",
        "prebuild": "rimraf ./dist",
        "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
        "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
        "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
        "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
    },
    "dependencies": {
        "debounce": "^1.2.1",
        "vuejs-paginate": "^2.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@babel/preset-env": "^7.14.7",
        "@fortawesome/fontawesome-free": "^6.1.2",
        "@rollup/plugin-alias": "^3.1.2",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^14.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@rollup/plugin-replace": "^2.4.2",
        "@vue/cli-plugin-babel": "^4.5.13",
        "@vue/cli-service": "^4.5.13",
        "axios": "^0.27.2",
        "bootstrap": "^3.4.1",
        "cross-env": "^7.0.3",
        "minimist": "^1.2.5",
        "rimraf": "^3.0.2",
        "rollup": "^2.52.8",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-vue": "^5.1.9",
        "vue": "^2.6.14",
        "vue-template-compiler": "^2.6.14"
    },
    "peerDependencies": {
        "vue": "^2.6.14"
    },
    "engines": {
        "node": ">=12"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/HELMAB/lv2-datatable.git"
    },
    "keywords": [
        "datatable",
        "laravel-datatable",
        "vue-datatable",
        "vue-laravel-datatable",
        "laravel-vue-datatable"
    ],
    "author": "HEL Mab",
    "license": "MIT License",
    "bugs": {
        "url": "https://github.com/HELMAB/lv2-datatable/issues"
    },
    "homepage": "https://github.com/HELMAB/lv2-datatable#README"
}
